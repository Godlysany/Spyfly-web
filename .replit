modules = ["web", "nodejs-20", "python-3.11", "postgresql-16"]

[nix]
channel = "stable-25_05"

[agent]
expertMode = true
integrations = ["javascript_supabase:1.0.0"]

[workflows]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 45455
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
run = ["python3", "-m", "http.server", "5000", "--bind", "0.0.0.0"]
